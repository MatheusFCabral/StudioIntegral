{
  "hosting": {
    "public": "dist/studio-integral",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "redirects": [{
      "source": "www.studiointegral.com.br/**",
      "destination": "https://studio-integral.com.br",
      "type": 302
    } ],
    "rewrites": [
      {
        "source": "**",
        "destination": "/index.html"
      }
    ]
  }
}
